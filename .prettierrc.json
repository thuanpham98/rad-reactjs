{
    "singleQuote": false,
    "bracketSpacing": true,
    "trailingComma": "all",
    "endOfLine": "crlf",
    "tabWidth": 2,
    "jsxSingleQuote": false,
    "arrowParens": "always",
    "bracketSameLine": false,
    "embeddedLanguageFormatting": "off",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleAttributePerLine": false,
    "useTabs": false
}